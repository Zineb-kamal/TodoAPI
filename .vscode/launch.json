    {
        "version": "0.2.0",
        "profiles": {
  "TodoAPI": {
    "commandName": "Project",
    "dotnetRunMessages": true,
    "launchBrowser": true,
    "applicationUrl": "https://localhost:5208;http://localhost:5207",
    "environmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    }
  }
},

        "configurations": [
          
          {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
          },
          {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
          },
          {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net6.0/YourProject.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "serverReadyAction": {
              "action": "openExternally",
              "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "environmentVariables": {
              "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceMaps": false,
            "justMyCode": true
          }
        ]
      }
      
